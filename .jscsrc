{
	"excludeFiles":[ "node_modules/**",".idea",".git","static"],
	"disallowArrowFunctions":false,
 	"disallowMultipleLineBreaks": false,
	"disallowTrailingComma":true,
	"disallowEmptyBlocks": true,
	"disallowSpaceAfterObjectKeys": true,
	"disallowCommaBeforeLineBreak": false,
	"disallowTrailingWhitespace": true,
	"disallowSpaceAfterPrefixUnaryOperators": [ "++", "--", "+", "-", "~", "!" ], "disallowSpaceBeforePostfixUnaryOperators": [ "++", "--" ],
	"disallowKeywordsOnNewLine": [ "else" ],
	"requireLineFeedAtFileEnd": true,
	"requireMultipleVarDecl": false,
	"requireCapitalizedConstructors": true,
	"requireSpacesInsideObjectBrackets": "all", "requireSpacesInsideArrayBrackets": "all",
	"requireSemicolons":true,
	"requireSpaceBeforeBinaryOperators": [ "+", "-", "/", "*", "=", "==", "===", "!=", "!==" ],
	"requireSpaceAfterBinaryOperators": [ "+", "-", "/", "*", "=", "==", "===", "!=", "!==" ],
	"requireSpaceAfterKeywords": [ "if", "else", "for", "while", "do", "switch", "try", "catch" ],
 	"requireSpaceBeforeBinaryOperators": [ "+", "-", "/", "*", "=", "==", "===", "!=", "!==" ],
	"requireSpaceAfterBinaryOperators": [ "+", "-", "/", "*", "=", "==", "===", "!=", "!==" ],
	"requireSpacesInFunctionExpression": { "beforeOpeningCurlyBrace": true },
	"fix":true,
	"esnext":true,
	"extract": ["*.htm", "*.html","*.ejs"],
	"validateLineBreaks": "CRLF",
}
