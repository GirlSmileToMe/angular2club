      <%if(user){%>
        global.__currentUser={
            _id:'<%=user._id%>',
            account:'<%=user.account%>',
            profile:{
              nickName:'<%=user.profile.nickName%>',
              avatar:'<%=user.profile.avatar%>'
            }
        };
      <%}%>
    })(window);


    angular.element(window).on('load', function () {
      angular.bootstrap(document,['app.starter']);
    });

